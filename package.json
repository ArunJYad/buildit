    "url": "https://github.com/buildit/buildit/issues"
  },
  "homepage": "https://github.com/buildit/buildit#readme",
  "devDependencies": {
    "chalk": "^1.1.3",
    "express": "^4.15.2",
    "recursive-copy": "^2.0.6",
    "browser-sync": "^2.13.0",
    "gulp": "^3.9.1",
    "gulp-bump": "^2.2.0",
    "gulp-concat": "^2.6.0",
    "gulp-csslint": "^0.3.1",
    "gulp-jshint": "^2.0.1",
    "gulp-plumber": "^1.1.0",
    "gulp-sourcemaps": "^1.6.0",
